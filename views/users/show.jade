extends ../layout

block content
	#wrapper
		h2.inline User List
		#userList
			table
				thead
					th UserName
					th Email
					th Immunity
					th Edit
					th Delete
				tbody
					tr
						td= username
						td= email
						td= immunity
						td
							a(href="/users/edit/"+userId) Edit
						td
							a(href="/users/delete/"+userId) Delete