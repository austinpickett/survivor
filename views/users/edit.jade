extends ../layout

block content		
	h1= title
	each user, i in edituser
		form#formAddUser(name="edituser",method="post",action="/users/edit/"+user._id)
			input#inputUserName(type="text", placeholder=user.username, name="username")
			input#inputUserEmail(type="text", placeholder=user.email, name="useremail")
			input#inputUserimmunity(type="text", placeholder=user.immunity, name="userimmunity")
			input#inputUserTimeOut(type="text", placeholder=user.timeOut, name="usertimeout")
			button(class="btn waves-effect waves-light" type="submit" name="action") Edit