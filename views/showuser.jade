extends layout

block content
	#wrapper
		h2.inline User List 
		a(href="/users/new")= "Add new user"
		#userList
			table
				thead
					th UserName
					th Email
					th Immunity
					th Edit
					th Delete
				tbody
					each user, i in showuser
						tr
							td= user.username
							td= user.email
							td= user.imunity
							td
								a(href="/users/edit/"+user._id) Edit
							td
								a(href="/users/delete/"+user._id) Delete